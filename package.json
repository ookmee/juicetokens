{
  "name": "token-engine",
  "version": "0.1.0",
  "description": "Token exchange protocol implementation",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run proto && tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "proto": "rimraf src/generated && mkdir -p src/generated && protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --js_out=import_style=commonjs,binary:./src/generated --ts_out=service=grpc-node:./src/generated ./protos/*.proto",
    "proto:watch": "nodemon --watch protos --ext proto --exec 'npm run proto'"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.8",
    "@grpc/proto-loader": "^0.7.10",
    "express": "^4.18.2",
    "google-protobuf": "^3.21.4",
    "ipfs-http-client": "^60.0.1",
    "mongodb": "^6.3.0",
    "grpc-web": "^1.4.2",
    "@improbable-eng/grpc-web": "^0.15.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/google-protobuf": "^3.15.12",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "^5.3.3",
    "rimraf": "^5.0.5",
    "nodemon": "^3.1.0"
  }
}
